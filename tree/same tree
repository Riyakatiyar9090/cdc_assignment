class Solution {
  public:
    bool help(Node* r1,Node* r2){
        if(r1==NULL || r2==NULL) return r1==r2;
        if(r1->data!=r2->data) return false;
        return help(r1->left,r2->left) && help(r1->right,r2->right);
    }
    bool isIdentical(Node* r1, Node* r2) {
        // code here
        return help(r1,r2);
    }
};
